[
    {
        "title": "Storages Overview",
        "type": "administrator",
        "table": "hvStorages",
        "url": "{}://{}:{}/v1/storage-systems",
        "jsonfilter": "f_response['resources']",
        "data": 
            [
                {
                "id": "1",
                "title": "Storage Information",
                "parameter":
                    {
                        "columnsFloat": 
                            [
                            ],
                        "columnsStr":
                            [
                                "storageSystemId",
                                "date",
                                "storageSystemName",
                                "model",
                                "svpIpAddress",
                                "gum1IpAddress",
                                "gum2IpAddress",
                                "firmwareVersion",
                                "horcmVersion"
                            ]
                    }
                },
                {
                    "id": "2",
                    "title": "Storage Capacity (GB)",
                    "parameter":
                        {
                            "columnsFloat": 
                                [
                                    "cacheCapacity",
                                    "totalUsableCapacity",
                                    "allocatedToPool",
                                    "unallocatedToPool",
                                    "usedCapacity",
                                    "availableCapacity",
                                    "subscribedCapacity",
                                    "unusedDisksCapacity"
                                ],
                            "columnsStr":
                                [
                                    "storageSystemId",
                                    "date"
                                ]
                        }
                    },
                    {
                        "id": "3",
                        "title": "Total Storage Efficiency (%)",
                        "parameter":
                            {
                                "columnsFloat": 
                                    [
                                        "dataReductionSavingsRate",
                                        "capacityEfficiencyRate",
                                        "totalEfficiency.totalEfficiencyRate.value",
                                        "totalEfficiency.dataReductionEfficiency.totalDataReductionRate.value",
                                        "totalEfficiency.snapshotEfficiencyRate.value",
                                        "totalEfficiency.provisioningEfficiencyPercentage.value"
                                    ],
                                "columnsStr":
                                    [
                                        "storageSystemId",
                                        "date"
                                    ]
                            }
                        },
                        {
                        "id": "4",
                        "title": "Storage Software Saving Efficiency (%)",
                        "parameter":
                            {
                                "columnsFloat": 
                                    [
                                        "softwareSavingEfficiency.totalSoftwareSavingRate.value",
                                        "softwareSavingEfficiency.compressionRate.value",
                                        "softwareSavingEfficiency.deduplicationRate.value",
                                        "softwareSavingEfficiency.patternMatchingRate.value"
                                    ],
                                "columnsStr":
                                    [
                                        "storageSystemId",
                                        "date"
                                    ]
                            }
                        },
                        {
                        "id": "5",
